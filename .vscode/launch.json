{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--disable-extension=ms-vscode.html-language-features",
				"--disable-extension=tencent-cloud.coding-copilot"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			]
		}
	]
}